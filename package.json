{
  "name": "ts-strictify",
  "version": "0.5.0",
  "description": "Enable gradual TypeScript strict",
  "keywords": [
    "typescript",
    "strict"
  ],
  "license": "MIT",
  "author": "Christian Schr√∂ter <dev@cschroeter.net> (https://cschroeter.net)",
  "bin": "./dist/bin.js",
  "engines": {
    "node": ">=8.10.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cschroeter/ts-strictify.git"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "tsc",
    "release": "release-it"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "execa": "^2.0.4",
    "simple-git": "^1.126.0",
    "yargs": "^14.0.0"
  },
  "peerDependencies": {
    "typescript": "3.x"
  },
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "@types/jest": "24.0.18",
    "@types/yargs": "13.0.3",
    "@typescript-eslint/eslint-plugin": "2.3.1",
    "@typescript-eslint/parser": "2.3.1",
    "eslint": "6.4.0",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "prettier": "1.18.2",
    "pretty-quick": "1.11.1",
    "release-it": "12.4.1",
    "typescript": "3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
